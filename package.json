{
  "name": "timeless",
  "author": "Christian Reinbacher",
  "version": "1.5.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "enamel": "^1.2.4",
    "pebble-clay": "^1.0.3"
  },
  "pebble": {
    "displayName": "Time Less",
    "uuid": "ce33feef-def1-4b3f-b5a7-612493b3cc2c",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "chalk",
      "basalt",
      "emery"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "border",
      "colorscheme",
      "animation",
      "shake"
    ],
    "resources": {
      "media": [
        {
          "type": "font",
          "characterRegex": "[0-9:]",
          "name": "FONT_DIGITAL_62",
          "file": "fonts/Quirkus.ttf"
        },
        {
          "memoryFormat": "1Bit",
          "menuIcon": true,
          "type": "bitmap",
          "name": "IMAGE_MENU_ICON",
          "file": "hourglass.png"
        }
      ]
    }
  }
}
